public class Ex2{
public static void main(String[] args){

int[] numeros ={4, 8, 16, 32, 64, 128}
int[] demon = {2, 0, 4, 8, 0}

for(int g=0;g<numeros.length;g++){
System.out.printl(numeros[g] +"/" + demon[g]+ "="+(numeros[g]/demon[g]) );
}
catch(ArithmeticException e){
System.out.printl("Erro ao dividir por zero");
}
catch(ArrayIndexOutOfBoundsException e){
System.out.printl("Posição do array inválido");
}

}
}
