public class Ex3{

public static void main (String[] args){

for(int g=0;g<numeros.length;g++){
System.out.printl(numeros[g] +"/" + demon[g]+ "="+(numeros[g]/demon[g]) );
}
catch(ArithmeticException e){
System.out.printl("Erro ao dividir por zero");
}
catch(ArrayIndexOutOfBoundsException e){
System.out.printl("Posição do array inválido");
}
finally{
System.out.println("Essa linha é impressa sempre depois do try ou catch");
}

}
}
