public class Ex4{
public static void main(String[] args){
int[] numeros ={4, 8, 16, 32, 64, 128}
int[] demon = {2, 0, 4, 8, 0}

for(int g=0;g<numeros.length;g++){
try{
System.out.printl(numeros[g] +"/" + demon[g]+ "="+(numeros[g]/demon[g]) );
}
catch(Exception e){
System.out.printl("e.getMessage()");
e.printStackTrace();
}
}
}
}
